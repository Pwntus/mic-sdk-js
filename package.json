{
  "name": "mic-sdk-js",
  "version": "3.0.2",
  "description": "MIC SDK for JavaScript.",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "test": "yarn lint && jest --coverage",
    "build-with-test": "yarn clean && yarn test && tsc && webpack",
    "build": "yarn clean && tsc && webpack",
    "clean": "rimraf lib lib-esm dist",
    "format": "tsfmt --useTsfmt tsfmt.json -r src/**/*.ts",
    "lint": "tslint 'src/**/*.ts' --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pwntus/mic-sdk-js.git"
  },
  "keywords": [
    "mic",
    "telenor",
    "start",
    "iot",
    "connexion"
  ],
  "author": "Pontus Aurdal <pontus.edvard@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pwntus/mic-sdk-js/issues"
  },
  "homepage": "https://github.com/Pwntus/mic-sdk-js#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@types/aws-iot-device-sdk": "^2.2.0",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.10.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^2.0.0",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.3333",
    "typescript-formatter": "^7.2.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "files": [
    "dist/**",
    "lib/**/*"
  ],
  "dependencies": {
    "aws-iot-device-sdk": "^2.2.1",
    "aws-sdk": "^2.419.0",
    "axios": "^0.18.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost/",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
